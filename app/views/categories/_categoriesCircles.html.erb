<section class="grid grid-cols-2 gap-8 p-4 text-center text-theme drop-shadow-lg">
  <% @category.each do |category| %>
    <!--    id:"<%#= dom_id category %>"-->
    <%= link_to category_path(category.id), class: "", id: dom_id(category) do %>
      <%= image_tag category&.photo, class: "clip-circle" if category.photo.attached? %>
      <p class="text-categoria">
        <%= category.name %>
      </p>
    <% end %>
  <% end %>
</section>